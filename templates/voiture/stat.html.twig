	<main id="main" class="main">
		<div class="pagetitle">
			<section class="section">
				<div class="row">
					<div class="col-lg-6 offset-lg-3">
						<div class="card">
							<div class="card-body">
								<div class="search-bar" style="position: absolute; margin-top: -4.6%; right: 4.5%"></div>
								<br>
								<center>
									<h1>Graphique des voitures
